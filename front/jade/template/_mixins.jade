mixin ex_js(url)
  script(src=url, type='text/javascript')

mixin js(url)
  script(src="js/#{url}.js", type='text/javascript')

mixin lib(url)
  script(src="lib/#{url}.min.js", type='text/javascript')

mixin css(url)
  link(type='text/css',href="css/#{url}.css",rel="stylesheet")

mixin ex_css(url)
  link(type='text/css',href=url,rel="stylesheet")
